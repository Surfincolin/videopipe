{{!-- /templates/stream/edit.hbs
 --}}
<h1>Edit Stream</h1>

<form role="form">
<div class="form-group">
  <label>ID:</label>
  {{input value=id class="form-control"}}
</div>
<div class="form-group">
  <label>Name:</label>
  {{input value=name class="form-control"}}
</div>
<div class="form-group">
  <label>rtmp:</label>
  {{input value=rtmp class="form-control"}}
</div>
<div class="form-group">
  <label>streamName:</label>
  {{input value=streamName class="form-control"}}
</div>
<div class="form-group">
  <label>description:</label>
  {{textarea value=description class="form-control"}}
</div>
<button type="button" class="btn btn-success" {{action "save"}}> save </button>
</form>
{{outlet}}